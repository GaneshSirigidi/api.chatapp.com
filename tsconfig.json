{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "es6",
    "sourceMap": true,
    "outDir": "./dist"
  },
  "allowJs": true,
  "noImplicitAny": false,
  "lib": [
    "es2015"
  ],
  "include": [
    "src",
    "src/helpers/.ts"
  ],
  "strict": false,
  "alwaysStrict": false
}